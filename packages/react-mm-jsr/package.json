{
  "name": "react-mm-jsr",
  "version": "2.0.0",
  "author": "Mateusz Koteja <soanvig@gmail.com>",
  "description": "React adapter to pure JS range input solution",
  "license": "LGPLv3",
  "main": "build/index.js",
  "types": "build/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/soanvig/mm-jsr.git"
  },
