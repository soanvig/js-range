    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "browser-env": "^3.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.19.0",
    "eslint-plugin-import-quotes": "^0.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.38.5",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^9.1.1",
    "ttypescript": "^1.5.12",
    "typescript": "^4.1.4"
  },
  "peerDependencies": {
    "mm-jsr": "~2.0.4",
    "react": "^17.0.2"
  },
  "ava": {
    "extensions": [
      "ts"
